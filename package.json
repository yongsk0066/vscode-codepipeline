{
  "name": "pipeline-viewer",
  "displayName": "pipeline_viewer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:dgu-sidebar",
    "onCommand:dgu-extension.testCase"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pipeline-list-viewer",
          "title": "파이프라인 뷰어",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "pipeline-list-viewer": [
        {
          "id": "pipeline-list",
          "name": "파이프라인 뷰어",
          "icon": "media/logo.svg",
          "contextualTitle": "Case Generator"
        }
      ]
    },
    "commands": [
      {
        "command": "pipeline-viewer.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "openPipeline",
        "title": "Your Command",
        "category": "Your Category"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "node-loader": "^2.0.0",
    "prettier": "^3.0.0",
    "source-map-loader": "^4.0.1",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@aws-sdk/client-codepipeline": "^3.370.0",
    "aws-sdk": "^2.1414.0"
  }
}
